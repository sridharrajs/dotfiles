{
  "rules": {
    "arrow-parens": [
      "warn",
      "as-needed"
    ],
    "quotes": [
      2,
      "single",
      "avoid-escape"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "no-console": [
      0
    ],
    "curly": [
      2,
      "all"
    ],
    "keyword-spacing": [
      2,
      {
        "before": true,
        "after": true
      }
    ],
    "semi": [
      2,
      "always"
    ],
    "comma-spacing": [
      2,
      {
        "after": true
      }
    ],
    "one-var": [
      2,
      "never"
    ],
    "camelcase": [
      2,
      {
        "properties": "never",
        "ignoreDestructuring": true
      }
    ],
    "no-with": 2,
    "semi-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "brace-style": [
      2,
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "block-scoped-var": "error",
    "array-bracket-spacing": [
      "error",
      "never"
    ],
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ],
    "no-restricted-syntax": 0,
    "object-curly-newline": 0,
    "arrow-body-style": 0,
    "comma-style": "error",
    "dot-notation": "error",
    "space-infix-ops": 2,
    "comma-dangle": [
      2,
      "always-multiline"
    ],
    "dot-location": [
      "error",
      "property"
    ],
    "arrow-spacing": [
      "error",
      {
        "before": true,
        "after": true
      }
    ],
    "prettier/prettier": "error"
  },
  "env": {
    "browser": false,
    "node": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "airbnb"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  }
}
